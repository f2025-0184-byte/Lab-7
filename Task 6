#include<iostream>
using namespace std;
int main() {
    int n;
    double harmonic = 0.0;
    cout<<"Enter the value of n:";
    cin>>n;
    int i;
    for(i=1; i<=n; i++) {
        harmonic+= 1.0 / i;
    }
    cout<<"The " << n << "the Harmonic number is : "<< harmonic<<endl;


    return 0;

}


